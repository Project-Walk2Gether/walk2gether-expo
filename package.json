{
  "name": "walk2gether",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "npx expo prebuild --clean",
    "build:prune-artifacts": "scripts/prune-artifacts.sh",
    "build:ios:production": "NODE_ENV=production eas build -p ios --local --clear-cache --profile production && npm run build:prune-artifacts",
    "build:ios:production:remote": "NODE_ENV=production eas build -p ios --clear-cache --profile production && npm run build:prune-artifacts",
    "publish:production": "eas update --branch production",
    "update-shared": "npm install walk2gether-shared git@github.com:Project-Walk2Gether/walk2gether-shared.git",
    "postinstall": "patch-package",
    "clear-all-caches": "rm -rf node_modules && npm cache clean --force && npm install && watchman watch-del-all && rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && npx expo start --clear"
  },
  "dependencies": {
    "@expo/config-plugins": "^9.0.17",
    "@expo/vector-icons": "^14.0.4",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-firebase/analytics": "^21.14.0",
    "@react-native-firebase/app": "^21.14.0",
    "@react-native-firebase/auth": "^21.14.0",
    "@react-native-firebase/crashlytics": "^21.14.0",
    "@react-native-firebase/firestore": "^21.14.0",
    "@react-native-firebase/functions": "^21.14.0",
    "@react-native-firebase/storage": "^21.14.0",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/material-top-tabs": "^7.2.10",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@tamagui/config": "^1.125.26",
    "@tamagui/lucide-icons": "^1.125.26",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "expo": "^52.0.46",
    "expo-background-fetch": "~13.0.6",
    "expo-build-properties": "~0.13.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "^17.0.7",
    "expo-device": "~7.0.3",
    "expo-font": "~13.0.4",

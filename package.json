    "react-native-slide-to-unlock": "^0.1.8",
    "react-native-svg": "15.8.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-uuid": "^2.0.3",
    "tamagui": "^1.125.26",
    "walk2gether-shared": "github:Project-Walk2Gether/walk2gether-shared",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/lodash": "^4.17.16",

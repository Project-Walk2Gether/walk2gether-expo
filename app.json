{
  "expo": {
    "name": "Walk2Gether",
    "slug": "walkmate",
    "version": "1.0.17",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcf0da"
    },
    "scheme": "walk2gether",
    "assetBundlePatterns": ["**/*", "assets/animations/*.lottie"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.projectwalk2gether.walk2gether",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow this app to access your location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow this app to access your location when using the app to create walks in your local area."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "org.projectwalk2gether.walk2gether"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "expo-updates",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-font"
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/7b384d5a-3e2e-4c79-895f-30255a67819a"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "7b384d5a-3e2e-4c79-895f-30255a67819a"
      },
      "googleMapsApiKey": "AIzaSyAjnbays5Hi7wYTj9PLljDDFJzZq3Dh3z8",
      "emulatorsEnabledInDev": true
    },
    "owner": "michaeldawson"
  }
}
